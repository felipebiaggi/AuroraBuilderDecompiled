<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Color x:Key="AuroraLightColor">#F7F7F7</Color>
    <Color x:Key="AuroraDarkColor">#151516</Color>
    <Color x:Key="AuroraWhiteColor">#FAFAFA</Color>
    <Color x:Key="AuroraBlackColor">#111112</Color>
    <Color x:Key="AuroraGoldColor">#B38000</Color>
    <Color x:Key="AuroraAccentColor">#305754</Color>
    <Color x:Key="AuroraColor">#305754</Color>
    <Color x:Key="AuroraBorealisColor">#305754</Color>
    <Color x:Key="AuroraDevelopmentColor">#7147AE</Color>
    <Color x:Key="SuccessColor">#37753D</Color>
    <Color x:Key="WarningColor">#CF6E39</Color>
    <Color x:Key="DangerColor">#943131</Color>
    <Color x:Key="SuccessDarkColor">#1D5338</Color>
    <Color x:Key="WarningDarkColor">#99512A</Color>
    <Color x:Key="DangerDarkColor">#802A2A</Color>
    <Color x:Key="SuccessBrightColor">#2F875B</Color>
    <Color x:Key="WarningBrightColor">#F08348</Color>
    <Color x:Key="DangerBrightColor">#BE3E3E</Color>
    <Color x:Key="RedDragonColor">#8E2626</Color>
    <Color x:Key="BlueDragonColor">#1D3953</Color>
    <Color x:Key="GreenDragonColor">#1D5338</Color>
    <Color x:Key="BlackDragonColor">#111111</Color>
    <Color x:Key="WhiteDragonColor">#FAFAFA</Color>
    <SolidColorBrush x:Key="AuroraLightBrush" options:Freeze="true" Color="{StaticResource AuroraLightColor}"/>
    <SolidColorBrush x:Key="AuroraDarkBrush" options:Freeze="true" Color="{StaticResource AuroraDarkColor}"/>
    <SolidColorBrush x:Key="AuroraWhiteBrush" options:Freeze="true" Color="{StaticResource AuroraWhiteColor}"/>
    <SolidColorBrush x:Key="AuroraBlackBrush" options:Freeze="true" Color="{StaticResource AuroraBlackColor}"/>
    <SolidColorBrush x:Key="AuroraGoldBrush" options:Freeze="true" Color="{StaticResource AuroraGoldColor}"/>
    <SolidColorBrush x:Key="AuroraAccentBrush" options:Freeze="true" Color="{StaticResource AuroraAccentColor}"/>
    <SolidColorBrush x:Key="AuroraDevelopmentBrush" options:Freeze="true"
                   Color="{StaticResource AuroraDevelopmentColor}"/>
    <SolidColorBrush x:Key="SuccessColorBrush" options:Freeze="true" Color="{StaticResource SuccessColor}"/>
    <SolidColorBrush x:Key="WarningColorBrush" options:Freeze="true" Color="{StaticResource WarningColor}"/>
    <SolidColorBrush x:Key="DangerColorBrush" options:Freeze="true" Color="{StaticResource DangerColor}"/>
    <SolidColorBrush x:Key="RedDragonColorBrush" options:Freeze="true" Color="{StaticResource RedDragonColor}"/>
    <SolidColorBrush x:Key="BlueDragonColorBrush" options:Freeze="true" Color="{StaticResource BlueDragonColor}"/>
    <SolidColorBrush x:Key="GreenDragonColorBrush" options:Freeze="true" Color="{StaticResource GreenDragonColor}"/>
    <SolidColorBrush x:Key="BlackDragonColorBrush" options:Freeze="true" Color="{StaticResource BlackDragonColor}"/>
    <SolidColorBrush x:Key="WhiteDragonColorBrush" options:Freeze="true" Color="{StaticResource WhiteDragonColor}"/>
    <SolidColorBrush x:Key="UpdateNotificationColorBrush" options:Freeze="true"
                   Color="{StaticResource SuccessBrightColor}"/>
    <Color x:Key="InvalidColor">#D042CF</Color>
    <SolidColorBrush x:Key="InvalidColorBrush" Color="{StaticResource InvalidColor}"/>
    <SolidColorBrush x:Key="AuroraGoldColorBrush" Color="{StaticResource AuroraGoldColor}"/>
    <SolidColorBrush x:Key="PinnedCharacterIconColorBrush" options:Freeze="True"
                   Color="{StaticResource AuroraGoldColor}"/>
    <SolidColorBrush x:Key="SuccessDarkColorBrush" Color="{StaticResource SuccessDarkColor}"/>
    <SolidColorBrush x:Key="AuroraBlackColorBrush" Color="{StaticResource AuroraBlackColor}"/>
    <SolidColorBrush x:Key="AuroraDarkColorBrush" Color="{StaticResource AuroraDarkColor}"/>
    <SolidColorBrush x:Key="AuroraLightColorBrush" Color="{StaticResource AuroraLightColor}"/>
    <SolidColorBrush x:Key="DragonHeadForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource RedDragonColor}"/>
    <SolidColorBrush x:Key="DeveloperIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource AuroraDevelopmentColor}"/>
    <SolidColorBrush x:Key="NewNotificationIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource SuccessColor}"/>
    <SolidColorBrush x:Key="ProgressIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource SuccessColor}"/>
    <SolidColorBrush x:Key="SettingsIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource AuroraWhiteColor}"/>
    <SolidColorBrush x:Key="WebIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource AuroraWhiteColor}"/>
    <SolidColorBrush x:Key="LoadingIndicatorForegroundColorBrush" options:Freeze="True"
                   Color="{StaticResource InvalidColor}"/>
    <Color x:Key="AuroraGraphicsButtonBorderBrushColor">#FF242425</Color>
    <Color x:Key="AuroraGraphicsButtonBorderBrushOverColor">#FF305754</Color>
    <SolidColorBrush x:Key="AuroraGraphicsButtonBorderBrush" options:Freeze="true"
                   Color="{DynamicResource AuroraGraphicsButtonBorderBrushColor}"/>
</ResourceDictionary>